    "expo": "^52.0.0",
    "expo-constants": "~17.0.4",
    "expo-font": "~13.0.3",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "iconsax-react-native": "^0.0.8",
